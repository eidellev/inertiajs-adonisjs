{
  "name": "@eidellev/inertia-adonisjs",
  "version": "2.1.0",
  "private": false,
  "description": "InertiaJS provider for AdonisJS",
  "repository": "https://github.com/eidellev/inertiajs-adonisjs",
  "bugs": "https://github.com/eidellev/inertiajs-adonisjs/issues",
  "main": "build/providers/InertiaProvider/index.js",
  "typings": "./build/adonis-typings/index.d.ts",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/instructions.js",
    "build/templates"
  ],
  "adonisjs": {
    "types": "@eidellev/inertia-adonisjs",
    "instructions": "./build/instructions.js",
    "preloads": [
      {
        "file": "./start/inertia",
        "environment": [
          "web"
        ]
      }
    ],
    "providers": [
      "@eidellev/inertia-adonisjs"
    ]
  },
  "license": "MIT",
  "scripts": {
    "lint": "tsc --noEmit && eslint . --ext=ts",
    "lint:fix": "eslint . --ext=ts --fix",
    "clean": "rimraf build",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "build": "cross-env yarn clean && yarn copyfiles && tsc",
    "watch": "cross-env yarn clean && yarn copyfiles && tsc -w",
    "test": "nyc node japaFile.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@adonisjs/mrm-preset": "github:adonisjs/mrm-preset",
    "@types/he": "^1.1.1",
    "he": "^1.2.0"
  },
  "peerDependencies": {
    "@adonisjs/config": ">=2",
    "@adonisjs/core": ">=5",
    "@adonisjs/validator": ">=9",
    "@adonisjs/view": ">=3"
  },
  "devDependencies": {
    "@adonisjs/config": "^3.0.0",
    "@adonisjs/core": "^5.0.1",
    "@adonisjs/require-ts": "^2.0.2",
    "@adonisjs/sink": "^4.2.2",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/view": "^3.1.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/prompt-cli": "^12.0.1",
    "@poppinss/dev-utils": "^1.1.0",
    "@types/common-tags": "^1.8.0",
    "@types/supertest": "^2.0.10",
    "adonis-preset-ts": "^2.0.0",
    "common-tags": "^1.8.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-adonis": "^1.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "japa": "^3.1.1",
    "lint-staged": "^10.5.4",
    "nyc": "^15.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "supertest": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
